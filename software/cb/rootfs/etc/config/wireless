config wifi-device  radio0
	option type     mac80211
	option channel  11
	option macaddr	c4:93:00:11:0d:38
	option hwmode	11b
	option txpower	10
	option htmode	HT20

config wifi-iface
	option device   radio0
#	option network  lan
	option network  wifi
	option mode     ap
	option ssid     luftschleuse
	option encryption none

